{
  "name": "tap-snack",
  "description": "Run a mobile tap builds in the browser using Appetize.io",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "apt:test": "node ./scripts/apt-test.js",
    "bs:test": "node ./scripts/bs-test.js",
    "copy:build": "cp -R ./build /keg/core-build",
    "task": "node ./tasks/runTask.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:tasks": "yarn jest --config ./configs/jest.config.js",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@keg-hub/appetite": "1.0.1",
    "@keg-hub/cli-utils": "0.2.2",
    "@keg-hub/git-lib": "0.0.1",
    "@keg-hub/jsutils": "8.5.0",
    "@keg-hub/spawn-cmd": "0.1.2",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "axios": "0.21.1",
    "dotenv": "9.0.2",
    "express": "^4.17.1",
    "form-data": "4.0.0",
    "module-alias-jest": "0.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "3.0.2",
    "web-vitals": "1.0.1"
  },
  "_aliases": {
    "SnackRoot": ".",
    "SnackTasks": "tasks"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
